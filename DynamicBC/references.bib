@book{Visintin2011,
abstract = {This book provides a systematic presentation of the extended models$\backslash$nof phase transition and of the mathematical investigation of the$\backslash$ncorresponding problems. The research in this field has experienced$\backslash$nan enormous development during the last two decades with an impressive$\backslash$namount of papers published at an ever increasing rate, making the$\backslash$nnecessity of an overview and of a comparison of the various theories$\backslash$nstrongly being felt among the specialists (mathematicians, physicists$\backslash$nand engineers). For such reason there was a strong expectation of$\backslash$na good book illustrating the various aspects of modelling phase change$\backslash$nprocesses and analysing the incredible variety of the mathematical$\backslash$nproblems which have been proposed in the literature. Certainly the$\backslash$nreaders will not be disappointed by Visintin's book, since with competence$\backslash$nand expository skill the author leads the reader through a logic$\backslash$nsequence of subjects of increasing physical and mathematical complexity.$\backslash$par$\backslash$nThe natural starting point is the Stefan problem and its different$\backslash$nformulations, discussing at length the nature of mushy regions. Then$\backslash$ngeneralizations such as kinetic undercooling, phase relaxation models,$\backslash$nand phase change of hetereogeneous materials are dealt with.$\backslash$par$\backslash$nModels based on a thermodynamical approach are illustrated in the$\backslash$nchapter devoted to the Gibbs-Thomson law and containing the presentation$\backslash$nof the phase field model. A further step is made considering phase$\backslash$nchange processes governed by nucleation and crystal growth, leading$\backslash$nto the analysis of mean curvature flows, hysteresis phenomena, and$\backslash$nother topics such as singular phase transitions, the Mullin-Sekerka$\backslash$nproblem, etc. An alternative nucleation model (non-adiabatic nucleation$\backslash$nwith small undercooling) is discussed in the last chapter with an$\backslash$napplication to the Landau-Lifshitz theory of ferromagnetism.$\backslash$par$\backslash$nA remarkable feature of this book is the great attention devoted$\backslash$nto the mathematical background. Indeed, the first part of the book$\backslash$ncontains an extensive summary of results about nonlinear PDEs of$\backslash$nthe kinds which appear most frequently in the sequel. The required$\backslash$nnon-elementary mathematical notions are explained in a long and detailed$\backslash$nappendix.$\backslash$par Although devoted to phase change, the book analyses$\backslash$nalso problems of different origin (e.g. the Muskat problem, the Hele-Shaw$\backslash$nproblem, etc.) whose mathematical structure is closely related to$\backslash$nphase change models.$\backslash$par The comments sections at the end of each$\backslash$nchapter are very useful not only because they provide a synoptic$\backslash$nview of the material but also for the guide to the quite extensive$\backslash$nliterature.},
author = {Visintin, Augusto},
booktitle = {Models of Phase Transitions},
