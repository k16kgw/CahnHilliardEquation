journal = {Computer Methods in Applied Mechanics and Engineering},
keywords = {Cahn-Hilliard,Isogeometric analysis,Isoperimetric problem,NURBS,Phase-field,Steady state solutions},
number = {49-50},
pages = {4333--4352},
