@article{Elliott1996a,
abstract = {The viscous Cahn-Hilliard equation may be viewed as a singular limit of the phase-field equations for phase transitions. It contains both the Allen-Cahn and Cahn-Hilliard models of phase separation as particular cases; by specific choices of parameters it may be formulated as a one-parameter (say $\alpha$) homotopy connecting the Cahn-Hilliard ($\alpha$ = 0) and Allen-Cahn ($\alpha$=1) models. The limit $\alpha$ = 0 is singular in the sense that the smoothing property of the analytic semigroup changes from being of the type associated with second order operators to the type associated with fourth order operators. The properties of the gradient dynamical system generated by the viscous Cahn-Hilliard equation are studied as $\alpha$ varies in [0, 1]. Continuity of the phase portraits near equilibria is established independently of $\alpha$ ∈ [0, 1] and, using this, a piecewise, uniform in time, perturbation result is proved for trajectories. Finally the continuity of the attractor is established and, in one dimension, the existence and continuity of inertial manifolds shown and the flow on the attractor detailed. {\textcopyright} 1996 Academic Press, Inc.},
author = {Elliott, C. M. and Stuart, A. M.},
doi = {10.1006/jdeq.1996.0101},
file = {:Users/keikagawa/Library/Application Support/Mendeley Desktop/Downloaded/Elliott, Stuart - 1996 - Viscous Cahn-Hilliard equation II. Analysis.pdf:pdf},
issn = {00220396},
journal = {Journal of Differential Equations},
number = {2},
pages = {387--414},
title = {{Viscous Cahn-Hilliard equation II. Analysis}},
volume = {128},
year = {1996}
