issn = {14320606},
journal = {SIAM J. Control Optim.},
keywords = {MPECs,dynamic boundary conditions,optimal control,optimality doncitions,parabolic obstacle problems},
number = {3},
pages = {2696--2721},
title = {{OPTIMAL BOUNDARY CONTROL OF A VISCOUS CAHN–HILLIARD SYSTEM WITH DYNAMIC BOUNDARY CONDITION AND DOUBLE OBSTACLE POTENTIALS}},
volume = {53},
year = {2015}
}
