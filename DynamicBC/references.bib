eprint = {0804.0988},
file = {:Users/keikagawa/Google ドライブ/Papers/Cahn-Hilliard/Grasselli,Schimperna,Segatti,Zelik-2009-On{\_}the{\_}3D{\_}Cahn-Hilliard{\_}equation{\_}with{\_}inertial{\_}term.pdf:pdf},
isbn = {0002800900177},
issn = {14243199},
keywords = {Cahn-Hilliard equation,Exponential attractors,Global attractors,Global existence,Weak solutions},
number = {2},
pages = {371--404},
title = {{On the 3D Cahn-Hilliard equation with inertial term}},
